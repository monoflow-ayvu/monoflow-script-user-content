{
  "name": "user-content",
  "version": "0.7.0",
  "description": "Habilitar/deshabilitar conteúdo para os usuários, baseado nos tags",
  "settingsSchema": {
    "display": "form",
    "settings": {
      "pdf": {
        "id": "1ec0f8ee-6685-5d98-a847-26f67b67d6f0",
        "src": "https://files.form.io/pdf/5692b91fd1028f01000407e3/file/1ec0f8ee-6685-5d98-a847-26f67b67d6f0"
      }
    },
    "components": [
      {
        "label": "Pages",
        "reorder": true,
        "addAnother": "Adicionar",
        "addAnotherPosition": "bottom",
        "layoutFixed": false,
        "enableRowGroups": false,
        "initEmpty": true,
        "tableView": false,
        "defaultValue": [
          {
            "tags": []
          }
        ],
        "key": "pages",
        "type": "datagrid",
        "input": true,
        "components": [
          {
            "label": "Tags",
            "tableView": false,
            "storeas": "array",
            "key": "tags",
            "type": "tags",
            "input": true
          },
          {
            "label": "Modo",
            "widget": "choicesjs",
            "tableView": true,
            "defaultValue": "show",
            "data": {
              "values": [
                {
                  "label": "Mostrar",
                  "value": "show"
                },
                {
                  "label": "Esconder",
                  "value": "hide"
                },
                {
                  "label": "Dock",
                  "value": "dock"
                }
              ]
            },
            "key": "mode",
            "type": "select",
            "input": true
          },
          {
            "label": "ID",
            "tableView": true,
            "multiple": true,
            "validate": {
              "required": true,
              "minLength": 3
            },
            "key": "id",
            "type": "textfield",
            "input": true
          }
        ]
      },
      {
        "type": "button",
        "label": "Submit",
        "key": "submit",
        "disableOnInvalid": true,
        "input": true,
        "tableView": false
      }
    ]
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --mode production --watch",
    "build-copy": "yarn build && (cat dist/bundle.js | pbcopy) && du -sh dist/*.js",
    "size": "npm run build && size-limit",
    "deploy": "node .scripts/deploy.js",
    "deploy-force": "node .scripts/deploy.js --force",
    "minor": "npm run build && bump minor --commit --tag --push --all",
    "release": "npm run build && bump major --commit --tag --push --all",
    "test": "jest --silent"
  },
  "browserslist": [
    "ie 6"
  ],
  "size-limit": [
    {
      "path": "dist/bundle.js",
      "limit": "100 kb",
      "gzip": false
    }
  ],
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/plugin-transform-arrow-functions": "7.23.3",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@jsdevtools/version-bump-prompt": "6.1.0",
    "@size-limit/preset-big-lib": "11.0.0",
    "@types/commander": "2.12.2",
    "@types/firebase": "3.2.1",
    "@types/jest": "29.5.10",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.0",
    "@types/uuid": "9.0.7",
    "babel-loader": "9.1.3",
    "brotli-webpack-plugin": "1.1.0",
    "commander": "11.1.0",
    "compression-webpack-plugin": "10.0.0",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "inquirer": "9.2.12",
    "jest": "29.7.0",
    "mobx-keystone": "1.7.1",
    "semver": "7.5.4",
    "size-limit": "11.0.0",
    "ts-loader": "9.5.1",
    "typed-emitter": "2.1.0",
    "typescript": "4.9.5",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "dependencies": {
    "@fermuch/monoutils": "0.2.0",
    "@fermuch/telematree": "9.23.0",
    "cross-fetch": "3.1.8",
    "firebase": "9.23.0",
    "lodash": "4.17.21",
    "mobx": "6.12.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/private/jest/setup.ts"
    ]
  }
}
